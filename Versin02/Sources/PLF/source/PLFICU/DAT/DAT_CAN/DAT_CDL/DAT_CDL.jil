//    };
//..
// The PRIORITY levels are defined by the Software Integrator in file lap_sup.jil.
// The lap_sup.jil file includes all xxx.jil files for the project.
// The PRIORITY rules for the CAN tasks of DAT_CAN:
// Priority that must be respect:     -1-      TOSClockSystem
//                                    -.-   
//                                    -i-      DATCnmTick  
//                                    -.-   
//                                    -i+j-    DATCdlCANTransmissionTask  
//                                    -.-   
//                                    -i+j+k-  DATCdlTaskReceptionCAN  
//                                    -.-   
//                                    -X-      Tasks LAP  
//------------------------------------------------------------------------------

    Task DATCdlCANTransmissionTask 
    {
        Fct = "DATCdlCANTransmissionTask";
        Id = "cTOSTaskID_EmissionCan";
        Priority = 75;
    };
    Task DATCdlCANReceptionTask 
    {
        Fct = "DATCdlCANReceptionTask";
        Id = "cTOSTaskID_ReceptionCan";
        Priority = 70;
    };

};

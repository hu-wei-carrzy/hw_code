  {
    Ctrl = "ControlName";
    Signal = SignalReference;
  }; 
  --------------------------------------------------------------------------*/
    OnControl = CallBack
    {
        Ctrl = "DATEnterActiveState";
        Fct = "DATDouEnterActiveState";
    };

    OnControl = CallBack
    {
        Ctrl = "DATLeaveActiveState";
        Fct = "DATDouEnterSleepMode";
    };
	

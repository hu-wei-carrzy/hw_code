//------------------------------------------------------------------------------
    ProdControl {Name = "LAPOpenDiagnosticSession" ;};
    ProdControl {Name = "LAPCloseDiagnosticSession";};
	//Communation Control
	ProdControl {Name = "LAPDiaAppEnableRx";};
	ProdControl {Name = "LAPDiaAppEnableTx";};
	ProdControl {Name = "LAPDiaAppDisableRx";};
	ProdControl {Name = "LAPDiaAppDisableTx";};
	ProdControl {Name = "LAPDiaNMEnableRx";};
	ProdControl {Name = "LAPDiaNMEnableTx";};
	ProdControl {Name = "LAPDiaNMDisableRx";};
	ProdControl {Name = "LAPDiaNMDisableTx";};
    ProdControl {Name = "LAPDiaSMLoadNvMImmediate";};


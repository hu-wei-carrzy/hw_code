    Graph LAPDiaTreatRequest 
    {
        Id = "cTOSSeqLAPDiaTreatRequest";
        GraphGroup = IdleGraphGroup;
    };

    Graph LAPDiaTreatService 
    {
        Id = "cTOSSeqLAPDiaTreatService";
        GraphGroup = IdleGraphGroup;
    };

    Graph LAPDiaCommunicationTimeOut 
    {
        Id = "cTOSSeqLAPDiaCommunicationTimeOut";
        GraphGroup = IdleGraphGroup;
    };

    Graph LAPDiaP2TimeOut 
    {
        Id = "cTOSSeqLAPDiaP2TimeOut";
        GraphGroup = IdleGraphGroup;
    };

	

//------------------------------------------------------------------------------

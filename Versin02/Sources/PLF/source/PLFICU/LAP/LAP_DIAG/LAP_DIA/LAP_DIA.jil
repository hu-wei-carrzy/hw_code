    {
        Ctrl = "LAPInitialize";
        Fct = "LAPDiaInitialize";
    };

    OnControl = CallBack
    {
        Ctrl = "LAPEnterActiveState";
        Fct = "LAPDiaStart";
    };

    OnControl = CallBack
    {
        Ctrl = "LAPLeaveActiveState";
        Fct = "LAPDiaStop";
